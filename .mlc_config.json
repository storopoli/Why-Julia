{
  "aliveStatusCodes": [403, 200, 206],
  "httpHeaders": [
    {
      "urls": [
        "https://github.com/",
        "https://guides.github.com/",
        "https://help.github.com/",
        "https://docs.github.com/"
      ],
      "headers": {
        "Accept-Encoding": "zstd, br, gzip, deflate"
      }
    }
  ],
  "retryOn429": true,
  "ignorePatterns": [
    {
      "pattern": "../.*"
    },
    {
      "pattern": "/github/workspace/.*"
    }
  ]
}
